<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#fa9333" />
            <gradient android:angle="-90" android:startColor="#fdc13e" android:endColor="#f29044"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#f9a62b" />
            <solid android:color="#f79d4e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#fca727" />
            <gradient android:angle="-90" android:startColor="#f79752" android:endColor="#f7c245" />
        </shape>
    </item>
</selector>